    cout<<"negyedikSzam = "<< negyedikSzam<<endl;
     cout<<"negyedikSzam cim = "<<&negyedikSzam<<endl;


    cout<<"----||-------"<<endl;
 harmadikSzam=masodikSzam;

      cout<<"elszoSzam = "<< elsoSzam<<endl;
     cout<<"elsoSzam cim = "<<&elsoSzam<<endl;
