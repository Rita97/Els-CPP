    cout<<"negyedikSzam = "<< negyedikSzam<<endl;
     cout<<"negyedikSzam cim = "<<&negyedikSzam<<endl;


     cout<<"----||-------"<<endl;


for(int i=0;i<TOMBMERET;i++){
    tomb[i]=i;
}


for(int i=0;i<TOMBMERET;i++){
    cout<<"Tomb"<<i<<" = "<<tomb[i];
    cout<<" \t\t A cime = "<<&tomb[i]<<endl;
