      harmadikSzam=negyzet(negyedikSzam);

           cout<<"elszoSzam = "<< elsoSzam<<endl;
     cout<<"elsoSzam cim = "<<&elsoSzam<<endl;
     cout<<"masodikSzam = "<< masodikSzam<<endl;
