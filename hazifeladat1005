#include <iostream>
#define TOMBMERET 20

using namespace std;

 int tomb[TOMBMERET];


bool duplazzmeg(int engem){
    engem=engem;
    return true ;

}
bool negyzet(int szam){
szam=szam*szam;
}

int main()
{
    int elsoSzam=10;
    int masodikSzam=20;
    int harmadikSzam;
    int negyedikSzam;




     cout<<"elszoSzam = "<< elsoSzam<<endl;
     cout<<"elsoSzam cim = "<<&elsoSzam<<endl;
