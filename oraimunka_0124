using namespace std;

int main()
{
	char tomb[100] = "orqipenwabhqxygapjtuevfufnsyfwkibhlfrzpefpnisqnvmbtcbogcqtspednjbpbghahujfrxobntdedydyjlzrmaljeneow";
		for (int i = 0; i < 100; i++) {
			if (tomb[i] == 't') {
				 tomb[i]='\t';
			}
			else if (tomb[i] == 'm') {
				tomb[i] ='\n';
			}
			
		}
		for (int i = 0; i < 100; i++) {
			cout << tomb[i];
		}

	std::cout << "Hello World!\n";
}
