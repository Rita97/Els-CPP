#include <iostream>
using namespace std;

int sum(int szam){
    int eredmeny=0;
    for(int i=1;i<szam+1;i++){
        eredmeny=eredmeny+i;
    }
    return eredmeny;
}
int sum(int kezdo, int vegso){
    int eredmeny=0;
    for(int i=kezdo;i<vegso+1;i++){
        eredmeny=eredmeny+i;
    }
    return eredmeny;

}
int faktorialis(int szam){
    int eredmeny=1;
    for(int i=1;i<szam+1;i++){
        eredmeny=eredmeny*i;
    }
    return eredmeny;
}
int main()
{
std::cout <<  sum(5);
std::cout <<  sum(2,8);
std::cout <<  faktorialis(4);
    return 0;
}
